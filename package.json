{
  "name": "mean-stack-ionic-boilerplate",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && node app.js",
    "dev": "concurrently \"nodemon --exec \"\"node -r dotenv\\config\"\" app.js\" \"npm run serve\"",
    "serve": "npm run ionic:serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --prod --wwwDir ./www",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build --prod --wwwDir ./www",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@ionic-native/core": "^4.7.0",
    "@ionic-native/geolocation": "^4.7.0",
    "@ionic-native/google-maps": "^4.7.0",
    "@ionic-native/splash-screen": "4.7.0",
    "@ionic-native/status-bar": "4.7.0",
    "@ionic/pro": "1.0.20",
    "@ionic/storage": "2.1.3",
    "body-parser": "^1.18.2",
    "cordova-plugin-ionic": "^4.1.7",
    "express": "^4.16.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "mongoose": "^5.0.11",
    "rxjs": "5.5.10",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "nodemon": "^1.17.2",
    "@ionic/app-scripts": "3.1.9",
    "typescript": "~2.6.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-ionic": {
        "APP_ID": "3dbd2628",
        "CHANNEL_NAME": "Production",
        "UPDATE_METHOD": "background"
      }
    }
  },
  "config": {
    "ionic_src_dir": "client"
  }
}
